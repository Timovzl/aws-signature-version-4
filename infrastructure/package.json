{
  "name": "signature-version-4-infrastructure",
  "version": "1.0.0",
  "license": "Apache-2.0",
  "bin": {
    "infrastructure": "bin/infrastructure.js"
  },
  "scripts": {
    "clean": "rimraf \"./cdk.out\" \"./+(bin|lib)/**/*.+(d.ts|js|js.map)\"",
    "build": "yarn clean && tsc",
    "watch": "tsc -w",
    "cdk": "cdk",
    "deploy": "yarn build && cdk deploy",
    "destroy": "yarn build && cdk destroy",
    "lint": "eslint --ext .ts --max-warnings=0 . && prettier --check .",
    "lint:fix": "eslint --ext .ts --fix . && prettier --write ."
  },
  "dependencies": {
    "@aws-cdk/assert": "2.0.0-rc.1",
    "@types/node": "14.14.44",
    "@typescript-eslint/eslint-plugin": "4.22.1",
    "@typescript-eslint/parser": "4.22.1",
    "aws-cdk-lib": "2.0.0-rc.1",
    "constructs": "10.0.5",
    "eslint": "7.26.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "source-map-support": "0.5.19",
    "typescript": "4.2.4"
  },
  "resolutions": {
    "bl": ">=4.0.3",
    "lodash": ">=4.17.13",
    "minimist": ">=1.2.2",
    "netmask": ">=2.0.1"
  }
}
